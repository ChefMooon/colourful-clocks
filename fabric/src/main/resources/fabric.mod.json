{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "issues": "${mod_issues}",
    "sources": "${mod_github}"
  },
  "license": "${mod_license}",
  "icon": "assets/colourfulclocks/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.chefmooon.colourfulclocks.fabric.ColourfulClocksFabric"
    ],
    "client": [
      "com.chefmooon.colourfulclocks.client.fabric.ColourfulClocksClientImpl"
    ],
    "fabric-datagen": [
      "com.chefmooon.colourfulclocks.data.fabric.DataGeneration"
    ],
    "rei_client": [
      "com.chefmooon.colourfulclocks.integration.rei.fabric.ClientREIPluginImpl"
    ],
    "jei_mod_plugin": [
      "com.chefmooon.colourfulclocks.integration.jei.fabric.JEIPluginImpl"
    ],
    "jade": [
      "com.chefmooon.colourfulclocks.integration.jade.JadePlugin"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabric-api": "*",
    "minecraft": ">=${minecraft_min_version} <=${minecraft_max_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.wiki": "${mod_homepage}",
        "modmenu.discord": "${mod_discord}"
      }
    }
  }
}